{
    "name": "wilbur-yu/hyperf-cache-ext",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "throttle",
        "php rate limiter",
        "php counter rate limiter"
    ],
    "description": "hyperf cache counter rate limiter",
    "autoload": {
        "psr-4": {
            "WilburYu\\HyperfCacheExt\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "hyperf/cache": "^2.2",
        "hyperf/di": "^2.2",
        "hyperf/exception-handler": "^2.2",
        "hyperf/redis": "^2.2",
        "hyperf/utils": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": ">=7.0",
        "swoole/ide-helper": "^4.5",
        "swow/swow": "dev-develop"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml --colors=always",
        "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "WilburYu\\HyperfCacheExt\\ConfigProvider"
        }
    }
}
